<?xml version="1.0" encoding="utf-8" ?>
<widget id="com.trackdrills.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>trackdrills</name>
  <description>
      A workout tracker application
  </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
      Apache Cordova Team
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <plugin name="cordova-plugin-googleplus" source="npm">
    <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.970094315674-4edd10559s8so2n023ecnvevogpcf8gh" />
    <variable name="WEB_APPLICATION_CLIENT_ID" value="970094315674-4edd10559s8so2n023ecnvevogpcf8gh.apps.googleusercontent.com" />
  </plugin>
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
</widget>